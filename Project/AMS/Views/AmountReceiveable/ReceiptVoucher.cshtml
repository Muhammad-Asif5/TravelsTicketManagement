@{
    Layout = "~/Views/Shared/_Admin.cshtml";
}
<style>
    nav {
        display: none !important;
    }

    aside {
        display: none !important;
    }

    footer {
        display: none !important;
    }
</style>
<br />
<div class="row">
    <div class="col-12">
        <div class="invoice p-3 mb-3">
            <div id="printableArea">
                <div class="row invoice-info">
                    <div class="col-sm-12 invoice-col">
                        <table class="" style="width:100%">
                            <thead>
                                <tr>
                                    <td colspan="2">
                                        <p style="font-size:14pt; font-weight:bolder"><i class="fas fa-globe"></i> Cash/Bank Receipt Voucher</p>
                                    </td>
                                    <td><strong>Receipt Date:</strong></td>
                                    <td>
                                        @ViewBag.Received_Paid_Date.ToString("dd-MMM-yyyy")
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="4" height="50">
                                        <img src="http://erpadmin.brillanttravel.com/Logo/logo.png" />
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">N 1781, Sur O’avenue Kapenda, Quarter Makutano, Commune et Ville de Lubumbashi</td>
                                    <td><strong>Receipt #</strong></td>
                                    <td>@ViewBag.VID</td>
                                </tr>
                                <tr>
                                    <td colspan="2"></td>
                                    <td><strong>Cust Code:</strong> </td>
                                    <td>@ViewBag.Cust_Code</td>
                                </tr>


                                <tr>
                                    <td colspan="4">
                                        <table style="width:100%">
                                            <tr>
                                                <td style="width:150px;"><strong>Customer Code:</strong></td>
                                                <td colspan="3"><strong>@ViewBag.Cust_Code</strong></td>
                                            </tr>
                                            <tr>
                                                <td><strong>Customer Name:</strong></td>
                                                <td colspan="3">@ViewBag.Contact_Name</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Address:</strong></td>
                                                <td colspan="3">@ViewBag.Address</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Received From:</strong></td>
                                                <td colspan="3">@ViewBag.Received_Paid</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Narration:</strong></td>
                                                <td colspan="3">@ViewBag.Narration</td>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                    </div>
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table">
                                <tr>
                                    <th colspan="2"><br />Stamp & Signature:______________________________</th>
                                    <td><br /></td>
                                    <th style="text-align:right"><br />Amount: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$ &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @ViewBag.Amount.ToString("N2")</th>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row no-print">
                <div class="col-12">
                    <a href="javascript:0" onclick="printDiv('printableArea')" class="btn btn-default"><i class="fas fa-print"></i> Print</a>
                    <button onclick="tableToExcel('printableArea', 'AR Receipt')" type="button" class="btn btn-primary float-right" style="margin-right: 5px;">
                        <i class="fas fa-download"></i> Generate Excel
                    </button>
                </div>
            </div>


        </div>
    </div>
</div>



